{
  "name": "strapi-plugin-bulk-editor",
  "version": "0.1.4",
  "description": "Bulk edit Strapi entries in a spreadsheet-like interface",
  "license": "MIT",
  "author": "John Chedeville <hi@jchd.me>",
  "repository": {
    "type": "git",
    "url": "https://github.com/JTCHE/strapi-plugin-bulk-editor"
  },
  "keywords": [
    "strapi",
    "strapi-plugin",
    "bulk-edit",
    "spreadsheet",
    "batch-update"
  ],
  "strapi": {
    "name": "bulk-editor",
    "displayName": "Bulk Editor",
    "description": "Edit multiple entries at once in a spreadsheet view",
    "kind": "plugin"
  },
  "main": "./strapi-server.js",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "source": "./admin/src/index.tsx",
      "import": "./strapi-admin.js",
      "require": "./strapi-admin.js",
      "default": "./strapi-admin.js"
    },
    "./strapi-server": {
      "source": "./server/index.js",
      "import": "./strapi-server.js",
      "require": "./strapi-server.js",
      "default": "./strapi-server.js"
    }
  },
  "files": [
    "admin",
    "server",
    "strapi-admin.js",
    "strapi-server.js"
  ],
  "dependencies": {},
  "peerDependencies": {
    "@strapi/strapi": "^5.0.0"
  }
}
